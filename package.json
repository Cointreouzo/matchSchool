  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "start:standalone": "NODE_ENV=production node .next/standalone/server.js",
    "railway:build": "NODE_ENV=production && rm -rf node_modules/.prisma && npm cache clean --force && npm install --platform=linux --arch=x64 && NEXT_DISABLE_SOURCEMAPS=true npx prisma generate && NEXT_DISABLE_SOURCEMAPS=true npx next build",
    "railway:start": "NODE_ENV=production PRISMA_SCHEMA_DISABLE_MIGRATIONS=true node server.js",
    "aliyun:build": "npm install && npm run build",
    "aliyun:start": "npx prisma generate && npx prisma migrate deploy && npm run start",
    "prod-test": "next build && npm run copy-static && NODE_ENV=production node server.js",
    "copy-static": "powershell -Command \"New-Item -ItemType Directory -Force -Path .next/standalone/.next/static; Copy-Item -Path .next/static/* -Destination .next/standalone/.next/static -Recurse -Force\"",
    "lint": "next lint",
    "check": "powershell -Command \"$date = Get-Date -Format 'yyyy-MM-dd-HH-mm'; mkdir -Force docs/lint-errors; cmd /c 'prisma generate && tsc --noEmit && next lint --strict && next build' 2>&1 | Tee-Object -FilePath docs/lint-errors/check-$date.md\"",
    "check-deploy": "powershell -Command \"Remove-Item -Recurse -Force node_modules, .next -ErrorAction SilentlyContinue; npm install; npm run check\"",
    "postinstall": "prisma generate"
  },
